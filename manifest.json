{
    "name": "ChromePHP",
    "version": "0.131",
    "content_scripts": [
      {
        "matches": ["http://*/*"],
        "js": ["log.min.js"]
      }
    ],
    "background_page": "background.html",
    "options_page": "options.html",
    "icons": { "16": "icon16.png",
               "48": "icon48.png",
              "128": "icon128.png" },
    "description": "For PHP logging in chrome console.",
    "browser_action": {
        "default_icon": "icon48.png",
        "popup": "popup.html"
    }
}
